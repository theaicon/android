<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/domainList"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:cacheColorHint="#00000000" android:layout_width="fill_parent"
	android:layout_height="?android:attr/listPreferredItemHeight"
	android:padding="6dip">

	<!-- Name -->
	<TextView android:id="@+id/name" android:text="TextView"
		android:singleLine="false" style="@style/Medium"
		android:layout_alignParentLeft="true" android:layout_toLeftOf="@+id/typeName"
		android:layout_margin="10dp"></TextView>

	<!-- Search Type Name -->
	<Button android:id="@+id/typeName" android:layout_height="wrap_content"
		style="@style/FreebaseButtonBlue" android:layout_width="wrap_content"
		android:text="Type" android:onClick="displayType"
		android:layout_alignTop="@+id/name" android:layout_alignParentRight="true"></Button>

</RelativeLayout>
