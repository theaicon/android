<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/childTypeRow"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:cacheColorHint="#00000000" android:layout_width="fill_parent"
	android:layout_height="?android:attr/listPreferredItemHeight"
	android:padding="6dip">

	<!-- property child text -->
	<TextView android:id="@+id/childPropertyText" android:text="TextView"
		android:singleLine="false" style="@style/Medium" android:textStyle="bold"
		android:layout_alignParentLeft="true" android:layout_margin="10dp"></TextView>

	<!-- property child id -->
	<TextView android:id="@+id/childPropertyId" android:text="TextView"
		android:singleLine="false" style="@style/Medium"
		android:layout_alignParentLeft="true" android:layout_below="@+id/childPropertyText"
		android:layout_margin="10dp"></TextView>

</RelativeLayout>
