<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/groupTypeRow"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:cacheColorHint="#00000000" android:layout_width="fill_parent"
	android:layout_height="?android:attr/listPreferredItemHeight"
	android:padding="6dip">

	<ImageView android:src="@drawable/freebase_collapsed"
		android:id="@+id/explist_indicator" android:layout_alignParentLeft="true"
		android:layout_width="25dp" android:layout_height="25dp" />

	<!-- Name -->
	<TextView android:id="@+id/groupTypeName" android:text="TextView"
		android:singleLine="false" style="@style/Large" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_alignBaseline="@id/explist_indicator"
		android:layout_toRightOf="@id/explist_indicator" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"></TextView>

</RelativeLayout>
